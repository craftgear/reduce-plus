{
  "name": "reduce-x",
  "version": "1.0.0",
  "description": "provides additional reduce functionalities",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "tsc --noEmit && eslint ./src/** &&  jest --maxWorkers=2 src ",
    "build": "tsc --declaration"
